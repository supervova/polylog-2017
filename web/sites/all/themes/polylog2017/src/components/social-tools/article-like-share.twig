
<aside class="social-options js-sticky">
  <button class="btn social facebook expanded" aria-label="Фейсбук — Нравится" title="Фейсбук — Нравится">
    <span class="fb-like" data-href="{{ post.link }}" data-layout="button" data-action="like" data-size="large" data-show-faces="false" data-share="false"></span>
  </button>

  <button class="btn social vk expanded" id="vk-like" aria-label="Вконтакте" title="Вконтакте">
    <img src="data:image/png;base64," alt="">

    {#
    <script type="text/javascript">
      VK.Widgets.Like("vk-like", {type: "mini", height: 30});
    </script> #}
  </button>

  <button class="btn social twitter expanded" data-href="https://twitter.com/home?status='{{ post.title | url_encode }}'+{{ post.link }}">
    <span class="icon twitter-xs" role="presentation"></span>Твитнуть
  </button>

  <button class="btn social telegram expanded" data-href="https://telegram.me/share/url?url={{ post.link }}" aria-label="Телеграм">
    <span class="icon telegram-xs" role="presentation"></span>Поделиться
  </button>

  <button class="btn social email expanded" data-href="mailto:?subject={{ post.title | url_encode }}&amp;body={{ post.link }}" aria-label="Почта">
    <span class="icon email-xs-white" role="presentation"></span>Поделиться
  </button>
</aside>
